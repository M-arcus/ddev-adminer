#!/bin/bash

## #ddev-generated: If you want to edit and own this file, remove this line.
## Description: Launch a browser with adminerevo
## Usage: adminerevo
## Example: "ddev adminerevo"

DDEV_ADMINER_PORT=9102
DDEV_ADMINER_HTTPS_PORT=9103

if [ ${DDEV_PRIMARY_URL%://*} = "http" ] || [ -n "${GITPOD_WORKSPACE_ID:-}" ] || [ "${CODESPACES:-}" = "true" ]; then
    ddev launch :$DDEV_ADMINER_PORT
else
    ddev launch :$DDEV_ADMINER_HTTPS_PORT
fi
